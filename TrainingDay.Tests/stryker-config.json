{
    "stryker-config":
    {
        "test-runner": "vstest",
        "reporters": [
            "progress",
            "html"
        ],
        "log-level": "info",
        "log-file":true,
        "timeout-ms": 10000,
        "max-concurrent-test-runners": 4,
        "threshold-high": 80,
        "threshold-low": 70,
        "threshold-break": 60,
        "file-patterns": [
            "!Program.cs"
        ],
        "ignore-methods": [
            "*Log*",
            "ToString",
            "*HashCode*"
        ]
    }
}